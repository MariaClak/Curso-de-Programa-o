Algoritmo "exercio6"
Var
 //variáveis do tipo real
    N1, N2, Resp : real
Inicio

       //entrada de dados
      escreva("Digite um valor: ")
      leia(N1)

      escreva("Digite outro valor: ")
      leia(N2)

      //processamento
      Resp <- N1 / N2

      //saída de dados
      escreval("Resultado = ", Resp)
Fimalgoritmo